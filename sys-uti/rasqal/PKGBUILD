# description	: Library that handles Resource Description Framework (RDF)
# depends	: libgcrypt libgmp util-linux bison flex raptor

pkgname=rasqal
pkgver=0.9.33
pkgrel=1
options=(!lto)
source=(https://download.librdf.org/source/$pkgname-$pkgver.tar.gz)

build() {
	cd $pkgname-$pkgver

    ./configure \
        --prefix=/usr \
        --disable-static
	make
    make DESTDIR="$pkgdir" install
}