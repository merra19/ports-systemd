# description	: Aspell (French) language dictionary
# depends	: which aspell

pkgname=aspell-fr
pkgver=0.50
pkgrel=1
source=(https://mirror.lyrahosting.com/gnu/aspell/dict/fr/aspell-fr-$pkgver-3.tar.bz2)

build() {
    cd aspell-fr-$pkgver-3

    ./configure
    make 
    make DESTDIR="$pkgdir" install
}
