# description	: Aspell (English) language dictionary
# depends	: which aspell

pkgname=aspell-en
pkgver=2020.12.07
pkgrel=1
source=(https://mirror.lyrahosting.com/gnu/aspell/dict/en/aspell6-en-$pkgver-0.tar.bz2)

build() {
	cd aspell6-en-$pkgver-0

    ./configure
    make
    make DESTDIR="$pkgdir" install
}
