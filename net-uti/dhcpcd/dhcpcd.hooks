#!/bin/sh

[ ! -d /var/lib/dhcpcd ] && install  -v -m700 -d /var/lib/dhcpcd
getent group dhcpcd > /dev/null || groupadd -g 52 dhcpcd
getent passwd dhcpcd > /dev/null || useradd  -c 'dhcpcd PrivSep' -d /var/lib/dhcpcd  -g dhcpcd  -s /bin/false  -u 52 dhcpcd

chown  -v dhcpcd:dhcpcd /var/lib/dhcpcd > /dev/null