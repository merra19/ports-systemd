# description	: KDE Framework 
# depends	: attica

pkgname=kapidox
pkgver=6.17.0
pkgrel=1
source=(https://download.kde.org/stable/frameworks/${pkgver%.*}/$pkgname-$pkgver.tar.xz)

build() {
    cd $pkgname-$pkgver

    gpep517 build-wheel --wheel-dir .dist --output-fd 3 3>&1 >&2
    python3 -m installer -d "$pkgdir" .dist/*.whl
}
