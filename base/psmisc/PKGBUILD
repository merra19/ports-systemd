# description	: A set of tools that use the proc filesystem
# depends	: ncurses

pkgname=psmisc
pkgver=23.7
pkgrel=1
source=(https://sourceforge.net/projects/psmisc/files/psmisc/$pkgname-$pkgver.tar.xz)

build() {
    cd $pkgname-$pkgver

    ./configure \
        --prefix=/usr
    make
    make DESTDIR="$pkgdir" install
}
